		<state readOnly="true" />
	</channel-type>

	<channel-type id="led">
		<item-type>Contact</item-type>
		<label>LED</label>
		<description>LED status.</description>
		
		<state readOnly="false" />
	</channel-type>

	<channel-type id="heater">
		<item-type>Contact</item-type>
