<?xml version="1.0" encoding="UTF-8"?>
<thing:thing-descriptions bindingId="tinkerforge" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:thing="http://eclipse.org/smarthome/schemas/thing-description/v1.0.0"
	xsi:schemaLocation="http://eclipse.org/smarthome/schemas/thing-description/v1.0.0 http://eclipse.org/smarthome/schemas/thing-description-1.0.0.xsd">


	<channel-type id="temperature">
		<item-type>Number:Temperature</item-type>
		<label>Temperature</label>
		<description>Current Temperature</description>
		<category>Temperature</category>
		<state readOnly="true" pattern="%.2f %unit%"/>
	</channel-type>

	<channel-type id="weight">
		<item-type>Number:Mass</item-type>
		<label>Weight</label>
		<description>Weight</description>
		<category>Pressure</category>
		<state readOnly="true" pattern="%.2f %unit%"/>
	</channel-type>

	<channel-type id="intensity">
		<item-type>Number</item-type>
		<label>Intensity</label>
		<description>The current sound intensity. The value has a range of 0 to 4095</description>
		
		<state readOnly="true" />
	</channel-type>

	<channel-type id="decibel">
		<item-type>Number:Dimensionless</item-type>
		<label>Decibel</label>
		<description>The measured sound pressure in decibels.</description>
		
		<state readOnly="true" pattern="%.1f %unit%"/>
	</channel-type>

	<channel-type id="illuminance">
		<item-type>Number:Illuminance</item-type>
		<label>Illuminance</label>
		<description>The current illuminance in lux.</description>
		
		<state readOnly="true" pattern="%.1f %unit%"/>
	</channel-type>

	<channel-type id="airpressure">
		<item-type>Number:Pressure</item-type>
		<label>Airpressure</label>
		<description>Barometric pressure</description>
		<category>Pressure</category>
		<state readOnly="true" pattern="%.1f %unit%"/>
	</channel-type>

	<channel-type id="altitude">
		<item-type>Number:Length</item-type>
		<label>Altitude</label>
		<description>The current altitude.</description>
		
		<state readOnly="true" pattern="%.1f %unit%"/>
	</channel-type>

	<channel-type id="distance">
		<item-type>Number:Length</item-type>
		<label>Distance</label>
		<description>The current distance.</description>
		
		<state readOnly="true" pattern="%.1f %unit%"/>
	</channel-type>

	<channel-type id="humidity">
		<item-type>Number:Dimensionless</item-type>
		<label>Humidity</label>
		<description>Humidity in percent.</description>
		<category>Humidity</category>
		<state readOnly="true" pattern="%.2f %unit%"/>
	</channel-type>

	<channel-type id="datetime">
		<item-type>DateTime</item-type>
		<label>DateTime</label>
		<description>Current Time and Date</description>
		<category>Time</category>
		<state readOnly="true" />
	</channel-type>

	<channel-type id="count">
		<item-type>Number</item-type>
		<label>Count</label>
		<description>The current value of a counter.</description>
		
		<state readOnly="true" />
	</channel-type>

	<channel-type id="voltage">
		<item-type>Number:ElectricPotential</item-type>
		<label>Voltage</label>
		<description>The current voltage in milli Volt.</description>
		
		<state readOnly="true" pattern="%.1f %unit%"/>
	</channel-type>

	<channel-type id="current">
		<item-type>Number:ElectricCurrent</item-type>
		<label>Current</label>
		<description>The current in milli Ampere.</description>
		
		<state readOnly="true" pattern="%.1f %unit%"/>
	</channel-type>

	<channel-type id="power">
		<item-type>Number:Power</item-type>
		<label>Power</label>
		<description>The power in Watt.</description>
		
		<state readOnly="true" pattern="%.1f %unit%"/>
